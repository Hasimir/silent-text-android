<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

  <plurals name="notify_new_messages_title">
    <item quantity="one">Получено новое сообщение</item>
    <item quantity="other"><xliff:g id="quantity">%1$d</xliff:g> новых сообщений</item>
  </plurals>

  <plurals name="notify_new_messages_subtitle">
    <item quantity="one"><xliff:g id="number_of_people">%1$d</xliff:g> человек отправил вам <xliff:g id="number_of_messages" example="2 messages">%2$s</xliff:g>.</item>
    <item quantity="other"><xliff:g id="number_of_people">%1$d</xliff:g> людей отправили вам <xliff:g id="number_of_messages" example="2 messages">%2$s</xliff:g>.</item>
  </plurals>

  <plurals name="n_messages">
      <item quantity="one">сообщение</item>
      <item quantity="other"><xliff:g id="count">%1$d</xliff:g> сообщений</item>
  </plurals>

  <plurals name="notify_unread_messages_from_individual">
    <item quantity="one"><xliff:g id="count">%1$d</xliff:g> непрочитанное сообщение от <xliff:g id="contact_name" example="droidman">%2$s</xliff:g></item>
    <item quantity="other"><xliff:g id="count">%1$d</xliff:g> непрочитанных сообщений от <xliff:g id="contact_name" example="droidman">%2$s</xliff:g></item>
  </plurals>

</resources>

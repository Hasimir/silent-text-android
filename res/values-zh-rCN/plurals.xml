<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

  <plurals name="notify_new_messages_title">
    <item quantity="one">收到新消息</item>
    <item quantity="other"><xliff:g id="quantity">%1$d</xliff:g> 新消息</item>
  </plurals>

  <plurals name="notify_new_messages_subtitle">
    <item quantity="one"><xliff:g id="number_of_people">%1$d</xliff:g> 人已发给您 <xliff:g id="number_of_messages" example="2 messages">%2$s</xliff:g>.</item>
    <item quantity="other"><xliff:g id="number_of_people">%1$d</xliff:g> 人们已发给您 <xliff:g id="number_of_messages" example="2 messages">%2$s</xliff:g>.</item>
  </plurals>

  <plurals name="n_messages">
      <item quantity="one">一个消息</item>
      <item quantity="other"><xliff:g id="count">%1$d</xliff:g> 消息</item>
  </plurals>

  <plurals name="notify_unread_messages_from_individual">
    <item quantity="one"><xliff:g id="count">%1$d</xliff:g> 条未读消息来自 <xliff:g id="contact_name" example="droidman">%2$s</xliff:g></item>
    <item quantity="other"><xliff:g id="count">%1$d</xliff:g> 条未读消息来自 <xliff:g id="contact_name" example="droidman">%2$s</xliff:g></item>
  </plurals>

</resources>

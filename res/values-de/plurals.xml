<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

  <plurals name="notify_new_messages_title">
    <item quantity="one">Neue Nachricht erhalten</item>
    <item quantity="other"><xliff:g id="quantity">%1$d</xliff:g> neue Nachrichten</item>
  </plurals>

  <plurals name="notify_new_messages_subtitle">
    <item quantity="one"><xliff:g id="number_of_people">%1$d</xliff:g> Person hat Ihnen <xliff:g id="number_of_messages" example="2 messages">%2$s</xliff:g>.</item>
    <item quantity="other"><xliff:g id="number_of_people">%1$d</xliff:g> Personen haben Ihnen <xliff:g id="number_of_messages" example="2 messages">%2$s</xliff:g>.</item>
  </plurals>

  <plurals name="n_messages">
      <item quantity="one">eine Nachricht</item>
      <item quantity="other"><xliff:g id="count">%1$d</xliff:g> Nachrichten</item>
  </plurals>

  <plurals name="notify_unread_messages_from_individual">
    <item quantity="one"><xliff:g id="count">%1$d</xliff:g> ungelesene Nachricht von <xliff:g id="contact_name" example="droidman">%2$s</xliff:g></item>
    <item quantity="other"><xliff:g id="count">%1$d</xliff:g> ungelesene Nachrichten von <xliff:g id="contact_name" example="droidman">%2$s</xliff:g></item>
  </plurals>

</resources>

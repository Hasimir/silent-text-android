<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

  <plurals name="notify_new_messages_title">
    <item quantity="one">Nova mensagem recebida</item>
    <item quantity="other"><xliff:g id="quantity">%1$d</xliff:g> novas mensagens</item>
  </plurals>

  <plurals name="notify_new_messages_subtitle">
    <item quantity="one"><xliff:g id="number_of_people">%1$d</xliff:g> pessoa enviou <xliff:g id="number_of_messages" example="2 messages">%2$s</xliff:g> para você.</item>
    <item quantity="other"><xliff:g id="number_of_people">%1$d</xliff:g> pessoas enviaram <xliff:g id="number_of_messages" example="2 messages">%2$s</xliff:g> para você.</item>
  </plurals>

  <plurals name="n_messages">
      <item quantity="one">uma mensagem</item>
      <item quantity="other"><xliff:g id="count">%1$d</xliff:g> mensagens</item>
  </plurals>

  <plurals name="notify_unread_messages_from_individual">
    <item quantity="one"><xliff:g id="count">%1$d</xliff:g> mensagem não lida de <xliff:g id="contact_name" example="droidman">%2$s</xliff:g></item>
    <item quantity="other"><xliff:g id="count">%1$d</xliff:g> mensagens não lidas de <xliff:g id="contact_name" example="droidman">%2$s</xliff:g></item>
  </plurals>

</resources>
